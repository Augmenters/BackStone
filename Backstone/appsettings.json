{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "ElasticConfiguration": {
    "Uri": "http://localhost:9200"
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "CacheSize": "1024",
    "SearchRadius": "700",
    "GridPrecision": "7",
    "PostGresHost": "database",
    "PostGresPort": "5432"
  },
  "ServiceEndpoints": {
    "YelpApiBaseUrl": "https://api.yelp.com/v3",
    "YelpGraphQLUrl": "https://api.yelp.com/v3/graphql"
  }
}

